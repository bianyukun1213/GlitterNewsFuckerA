.object {
    width: 8px;
    height: 50px;
    margin-right: 5px;
    background-color: #FFF;
    animation: animate 1s infinite;
    float: left;
}

.object:last-child {
    margin-right: 0px;
}

.object:nth-child(10) {
    animation-delay: 0.9s;
}

.object:nth-child(9) {
    animation-delay: 0.8s;
}

.object:nth-child(8) {
    animation-delay: 0.7s;
}

.object:nth-child(7) {
    animation-delay: 0.6s;
}

.object:nth-child(6) {
    animation-delay: 0.5s;
}

.object:nth-child(5) {
    animation-delay: 0.4s;
}

.object:nth-child(4) {
    animation-delay: 0.3s;
}

.object:nth-child(3) {
    animation-delay: 0.2s;
}

.object:nth-child(2) {
    animation-delay: 0.1s;
}

@-webkit-keyframes animate {
    50% {
        transform: scaleY(0);
    }
}

@keyframes animate {
    50% {
        transform: scaleY(0);
    }
}

